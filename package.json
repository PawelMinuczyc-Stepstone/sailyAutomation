{
  "name": "sailyautomation",
  "version": "1.0.0",
  "description": "Test automation for saily.com/pl using Playwright",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report reports/html-report",
    "codegen": "playwright codegen",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "test:snapshots": "playwright test tests/visual/snapshot.test.ts",
    "test:snapshots:update": "playwright test tests/visual/snapshot.test.ts --update-snapshots",
    "test:snapshots:headed": "playwright test tests/visual/snapshot.test.ts --headed"
  },
  "keywords": [
    "playwright",
    "testing",
    "automation",
    "e2e",
    "saily"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  }
}
